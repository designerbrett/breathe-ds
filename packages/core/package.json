{
  "name": "@breathe-ds/core",
  "version": "0.1.0",
  "description": "Breathe Design System - Core Design Tokens",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "react-native": "./dist/platforms/react-native.js",
  "exports": {
    ".": {
      "react-native": "./dist/platforms/react-native.js",
